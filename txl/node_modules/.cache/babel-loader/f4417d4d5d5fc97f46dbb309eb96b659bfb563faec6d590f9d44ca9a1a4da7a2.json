veEditContact() {\n      this.$refs.editContactForm.validate(valid => {\n        if (valid) {\n          const index = this.contacts.findIndex(c => c.phone === this.editContact.phone);\n          if (index !== -1) {\n            this.contacts.splice(index, 1, {\n              ...this.editContact\n            });\n            this.$message.success('修改联系人成功');\n            this.editDialogVisible = false;\n          }\n        } else {\n          this.$message.error('请检查输入信息');\n        }\n      });\n    },\n    deleteContact(contact) {\n      this.contacts = this.contacts.filter(c => c.phone !== contact.phone);\n      this.$message.success('联系人已删除');\n    },\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    }\n  }\n};","map":{"version":3,"names":["data","contacts","name","phone","email","searchKeyword","addDialogVisible","editDialogVisible","newContact","editContact","rules","required","message","trigger","pattern","type","computed","filteredContacts","filter","contact","includes","methods","showAddDialog","addContact","$refs","newContactForm","validate","valid","push","$message","success","resetForm","error","showEditDialog","saveEditContact","editContactForm","index","findIndex","c","splice","deleteContact","formName","resetFields"],"sources":["src/views/form.vue"],"sourcesContent":["<template>\r\n  <div class=\"contact-container\">\r\n    <!-- 搜索框 -->\r\n    <el-row>\r\n      <el-col :span=\"12\">\r\n        <el-input v-model=\"searchKeyword\" placeholder=\"请输入搜索关键字\" clearable></el-input>\r\n      </el-col>\r\n      <el-col :span=\"4\" :offset=\"8\" class=\"add-contact-btn\">\r\n        <el-button type=\"primary\" @click=\"showAddDialog\">添加联系人</el-button>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <!-- 通讯录列表 -->\r\n    <el-table :data=\"filteredContacts\" style=\"width: 100%; margin-top: 20px;\">\r\n      <el-table-column prop=\"name\" label=\"姓名\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话\" 